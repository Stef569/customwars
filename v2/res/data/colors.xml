<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ColorFilters [
        <!ELEMENT ColorFilters (ColorFilter)+>
        <!ELEMENT ColorFilter (Name , BaseColor , OriginalColors , IgnoredColors , ReplacementColor*)>
        <!ELEMENT Name (#PCDATA)>
        <!ELEMENT BaseColor (#PCDATA)>
        <!ELEMENT OriginalColors (#PCDATA)>
        <!ELEMENT IgnoredColors (#PCDATA)>
        <!ELEMENT ReplacementColor (Name , ReplacementColors)>
        <!ELEMENT ReplacementColors (#PCDATA)>
        ]>
<!-- Colors that can be used instead of their RGB values: WHITE,LIGHT_GRAY,GRAY,DARK_GRAY,BLACK,RED,PINK,ORANGE,YELLOW,GREEN,MAGENTA,CYAN,BLUE 
For example the unit baseColor could be replaced by RED
-->
<ColorFilters>
